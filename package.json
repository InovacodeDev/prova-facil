{
    "name": "prova-facil",
    "private": true,
    "version": "0.1.0",
    "devDependencies": {
        "turbo": "^2.5.8"
    },
    "packageManager": "pnpm@10.16.1",
    "scripts": {
        "build": "turbo run build",
        "build:web": "turbo run build --filter=@shop-wise/web",
        "build:vercel": "node build-for-vercel.js",
        "dev": "turbo run dev",
        "dev:web": "turbo run dev --filter=@shop-wise/web",
        "dev:api": "turbo run dev --filter=@shop-wise/api",
        "start": "turbo run start",
        "test": "turbo run test",
        "lint": "turbo run lint",
        "clean": "turbo run clean",
        "install:all": "pnpm install"
    }
}