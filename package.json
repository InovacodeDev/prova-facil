{
    "name": "prova-facil",
    "private": true,
    "version": "0.1.0",
    "devDependencies": {
        "@types/node": "^24.6.0",
        "dotenv": "^17.2.2",
        "turbo": "^2.5.8"
    },
    "packageManager": "pnpm@10.17.1",
    "scripts": {
        "build": "turbo run build",
        "build:web": "turbo run build --filter=@prova-facil/web",
        "build:vercel": "node build-for-vercel.js",
        "dev": "turbo run dev",
        "dev:web": "turbo run dev --filter=@prova-facil/web",
        "dev:api": "turbo run dev --filter=@prova-facil/api",
        "start": "turbo run start",
        "test": "turbo run test",
        "lint": "turbo run lint",
        "clean": "turbo run clean",
        "install:all": "pnpm install",
        "gen:words": "node ./scripts/generate-word-sequence.ts"
    }
}