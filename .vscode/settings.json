{
  "breadcrumbs.enabled": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.organizeImports": "always"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.insertSpaces": true,
  "editor.rulers": [80, 120],
  "editor.semanticHighlighting.enabled": true,
  "editor.tabSize": 2,
  "eslint.validate": ["javascript", "javascriptreact", "typescript", "typescriptreact"],
  "explorer.compactFolders": false,
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.expand": true,
  "explorer.fileNesting.patterns": {
    ".env.example": ".env, .env.*.example, .env.local",
    ".gitignore": ".vercelignore, .dockerignore, .eslintignore",
    "*.css": "${capture}.module.css, ${capture}.styles.css",
    "*.js": "${capture}.spec.js, ${capture}.test.js",
    "*.ts": "${capture}.d.ts, ${capture}.spec.ts, ${capture}.test.ts",
    "*.tsx": "${capture}.spec.tsx, ${capture}.test.tsx, ${capture}.stories.tsx",
    "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, pnpm-workspace.yaml",
    "tsconfig.json": "tsconfig.*.json, tsconfig.tsbuildinfo"
  },
  "explorer.openEditors.visible": 1,
  "files.associations": {
    ".env": "dotenv",
    ".env.*": "dotenv",
    "*.prisma": "prisma",
    "*.sql": "sql",
    "*.yaml": "yaml",
    "*.yml": "yaml",
    "Dockerfile*": "dockerfile"
  },
  "files.exclude": {
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.next": true,
    "**/.turbo": true,
    "**/.vercel": true,
    "**/coverage": true,
    "**/dist": true,
    "**/node_modules": true,
    "**/tmp": true
  },
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/.next/**": true,
    "**/dist/**": true,
    "**/node_modules/**": true
  },
  "git.autofetch": true,
  "git.confirmSync": false,
  "prettier.requireConfig": true,
  "search.exclude": {
    "**/.next": true,
    "**/coverage": true,
    "**/dist": true,
    "**/node_modules": true
  },
  "search.followSymlinks": false,
  "terminal.integrated.defaultProfile.osx": "zsh",
  "terminal.integrated.profiles.osx": {
    "zsh": {
      "path": "/bin/zsh"
    }
  },
  "typescript.tsdk": "node_modules/typescript/lib",
  "workbench.startupEditor": "none",
  "yaml.schemas": {
    "https://json.schemastore.org/github-workflow.json": ".github/workflows/*.{yml,yaml}",
    "https://raw.githubusercontent.com/compose-spec/compose/master/schema/compose-spec.json": [
      "docker-compose*.yml",
      "docker-compose*.yaml"
    ]
  }
}
